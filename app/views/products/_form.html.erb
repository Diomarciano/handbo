


<%= form_for Product.new , html: { multipart: true } do |f| %>
  <form>
  <div class="form-group">
    <div class="row">
      <div class="col-xs-6 .col-sm-4 sideleft">
        <div class="field">
          <%= f.label :"Nama Barang" %><br>
          <%= f.text_field :item,class:"form-control" %>
        </div>
      </div>
    </div>
  </div>
  </div>
  <div class="form-group">
    <div class="row">
      <div class="col-xs-6 .col-sm-4 sideleft">
        <%= f.label :"Rp Harga Sewa Per Hari" %>
        <%= f.text_field :price,class:'form-control',id:"inputrupiah" %>

      </div>
    </div>
  </div>
  <div class="container">
    <div class='col-xs-6 .col-sm-4 sideleft1'>
        <div class="form-group">
            <div class='input-group date'>
                <span class="input-group-addon">
                  <%= f.label :"Tanggal Sewa" %>
                  <%= f.datetime_select :from,id:'datetimepicker1'%>
                </span>
            </div>
        </div>
    </div>
  </div> 
  <div class="form-group">
    <div class="row">
      <div class="col-xs-6 .col-sm-4 sideleft">      
        <%= f.label :"Penjelasan Barang" %><br>
        <%= f.text_field :description,class:"form-control" %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="row">
     <div class="col-xs-6 .col-sm-4 sideleft"> 
       <%= f.label :"No.Handphone" %><br>
       <%= f.text_field :subitem,class:"form-control" %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="row">
      <div class="col-xs-6 .col-sm-4 sideleft">
        <%= f.label :Kota %><br>
        <%= f.text_field :stadt,class:"form-control" %>
      </div>
    </div>
  </div>
  <div class="form-group">
    <div class="row">
      <div class="col-xs-6 .col-sm-4 sideleft">
        <%= f.label :Gambar %>
        <%= f.file_field :image %>
      </div>
    </div>
  </div>
  <div class="form-group">
  <div class="row">
  <div class="col-xs-6 .col-sm-4 sideleft">
  <div class="actions">
    <%= f.submit "Pratinjau",class:"btn btn-default" %>
  </div>
</div>
</div>
</div>
</form>
</div>
</div>
</div>
</div>
</div>
<% end %>
</body>
</html>


