<!DOCTYPE html>
<html>
<head>
  <title><%= yield(:title) %></title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag  'application'  %>
  <%= csrf_meta_tags %>
</head>
<body>
    <% if current_user %>
      <p class="navbar-text navbar-right">Logged in as <%= current_user.email %>
        <%= link_to "Log out",logout_path %>
      </p>
    <% end %>


  <%= yield %>
   

</body>
</html>
